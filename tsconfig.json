{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "skipLibCheck": true,
      "lib": ["es2015", "dom"],
      "allowJs": true,
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "baseUrl": "./",
      "esModuleInterop": true
    },
    "include": ["*"],
    "exclude": ["node_modules"]
  }
  